<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.effect.ColorAdjust?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="650.0" prefWidth="1065.0" stylesheets="@application.css" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.dental.GUI.ControllerPopup">
   <children>
      <Button fx:id="btnBack" layoutX="60.0" layoutY="570.0" mnemonicParsing="false" onAction="#onClickBack" prefHeight="50.0" prefWidth="160.0" text="&lt;&lt; Back" textAlignment="CENTER">
         <font>
            <Font name="Consolas Bold" size="20.0" />
         </font>
         <tooltip>
            <Tooltip text="Click this button to return to the result stage" />
         </tooltip>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <MenuBar prefHeight="44.0" prefWidth="1065.0">
        <menus>
          <Menu fx:id="menuFile" mnemonicParsing="false" text="File">
            <items>
                  <MenuItem fx:id="itmSave" mnemonicParsing="false" text="Save" />
            </items>
          </Menu>
          <Menu fx:id="menuHelp" mnemonicParsing="false" onAction="#onClickHelp" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
         <effect>
            <ColorAdjust brightness="0.13" contrast="-0.05" hue="-0.1" saturation="-0.12" />
         </effect>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <tooltip>
            <Tooltip text="Click Save to save image, About for software information" />
         </tooltip>
      </MenuBar>
      <Button fx:id="btnExit" layoutX="860.0" layoutY="570.0" mnemonicParsing="false" onAction="#onClickExit" prefHeight="50.0" prefWidth="160.0" text="Exit" textAlignment="CENTER">
         <font>
            <Font name="Consolas Bold" size="20.0" />
         </font>
         <tooltip>
            <Tooltip text="Click this button to close the program" />
         </tooltip>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <HBox fx:id="Preview" alignment="CENTER" layoutX="170.0" layoutY="65.0" prefHeight="430.0" prefWidth="730.0" />
   </children>
</AnchorPane>
