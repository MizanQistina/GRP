<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.AreaChart?>
<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="654.0" prefWidth="1065.0" stylesheets="@application.css" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.dental.GUI.ControllerResult">
   <children>
      <MenuBar prefHeight="44.0" prefWidth="1065.0">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <BarChart layoutX="542.0" layoutY="140.0" prefHeight="400.0" prefWidth="450.0">
        <xAxis>
          <CategoryAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis prefWidth="18.0" side="LEFT" />
        </yAxis>
      </BarChart>
      <AreaChart layoutX="72.0" layoutY="140.0" prefHeight="400.0" prefWidth="450.0">
        <xAxis>
          <CategoryAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </AreaChart>
      <Button fx:id="btnBack" layoutX="98.0" layoutY="570.0" mnemonicParsing="false" onAction="#onClickBack" prefHeight="50.0" prefWidth="150.0" text="&lt;&lt; Back">
         <font>
            <Font name="Consolas Bold" size="20.0" />
         </font></Button>
      <Button fx:id="btnResult" layoutX="105.0" layoutY="63.0" mnemonicParsing="false" onAction="#onClickResults" prefHeight="50.0" prefWidth="160.0" text="Show Results" textAlignment="CENTER">
         <font>
            <Font name="Consolas Bold" size="20.0" />
         </font></Button>
   </children>
</AnchorPane>
